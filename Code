#include <iostream>
#include <string>
#include <vector>

using namespace std;

int main() {
    setlocale(LC_ALL, "Russian");
    vector <int> numbers;
    string str;
    getline(cin, str);
    int c = 0, tmp = 0;
    bool isinp = 0, mulpl = 0, div = 0;
    for (auto i : str)
    {
        if (48 <= i && i <= 57) {
            c = c * 10 + int(i) - 48;
            isinp = 1;
        }
        else if (isinp) {
            if (div) {
                numbers.push_back(c * tmp);

            }
            if (mulpl) {
                numbers.push_back(tmp / c);
            }
            numbers.push_back(c);
            c = 0;
            isinp = 0;
        }    
        if ('/' == i) {
            tmp = numbers.back();
            numbers.pop_back();
            div = 1;
        }
        else if ('*' == i) {
            tmp = numbers.back();
            numbers.pop_back();
            mulpl = 1;
        }
            
            // {1, 2, 7, 9, 40} tmp = 7; 7 * 88;
            // v.back()
            // v.pop_back()
            // 5 / 6
    }
    if (isinp)
        numbers.push_back(c);
}
